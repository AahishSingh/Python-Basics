    "## (B) Search Phone Number and e-mail"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 26,
   "id": "33b40f77",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "someone12@example.in\n",
      "+919876543210\n"
     ]
    }
   ],
   "source": [
    "import re\n",
    "\n",

    "phone_regex = re.compile(r'\\+\\d{12}')\n",
    "email_regex = re.compile(r'[A-Za-z0-9._]+@[A-Za-z0-9]+\\.[A-Z|a-z]{2,}')\n",

    "with open('a.txt', 'r') as f:\n",
    "    for line in f:\n",

    "        matches = phone_regex.findall(line)\n",
    "      
    "        for match in matches:\n",
    "            print(match)\n",
    "            \n",
    "        matches = email_regex.findall(line)\n",
    "       
    "        for match in matches:\n",
    "            print(match) "
   ]
  }
